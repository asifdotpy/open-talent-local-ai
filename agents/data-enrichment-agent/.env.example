# Proxycurl API (https://nubela.co/proxycurl/)
PROXYCURL_API_KEY=your_proxycurl_api_key_here

# Nubela API (https://nubela.co/nubela/)
NUBELA_API_KEY=your_nubela_api_key_here

# Google Custom Search Engine (https://developers.google.com/custom-search)
GOOGLE_CSE_API_KEY=your_google_api_key_here
GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id_here

# Redis (for production caching)
REDIS_URL=redis://localhost:6379

# Rate Limits (requests per minute)
PROXYCURL_RATE_LIMIT=100
NUBELA_RATE_LIMIT=200
GOOGLE_CSE_RATE_LIMIT=100

# Cache Settings
CACHE_TTL_DAYS=30
